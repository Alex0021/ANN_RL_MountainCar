    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run training",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--train"
            ],
        },
        {
            "name": "Run evaluation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--eval"
            ],
        },
        {
            "name": "Run generate plots",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/generate_plots.py",
            "args": [
                "--folder",
                "logs/train/2024-05-09-06-22-18"
            ],
        },
        {
            "name": "Run RND training",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--train",
                "--agent",
                "dqn-rnd"
            ],
        },   

    ]
}